
pipeline{
tools{
        jdk 'myjava'
        maven 'mymaven'
    }
    agent none

  stages{
     stage('checkout'){
     agent {label 'Node1'}
         steps{
            git 'https://github.com/Matrejeur/JavaWebCalculator.git'
         }
     }
     stage('Build'){
     agent any     
        steps{
         sh 'mvn clean install'
        }
     }
     stage ('Code Quality')
     {
         step{
         withSonarQubeEnv('SonarQube'){
         sh 'mvn sonar.sonar'
         }
         }
     }
  }

}
