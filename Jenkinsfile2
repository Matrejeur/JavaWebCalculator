
pipeline{
  stages{
     stage('checkout'){
     agent {label 'Node1'}
         steps{
            git 'https://github.com/Matrejeur/JavaWebCalculator.git'
         }
     }
     stage('Build'){
     agent any     
        steps{
         sh 'mvn clean install -f JavaWebCalculator/pom.xml'
        }
     }
  }

}
